{
  "name": "@mongosh/service-provider-server",
  "version": "1.1.0",
  "description": "MongoDB Shell Server Service Provider Package",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "config": {
    "unsafe-perm": true
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mongodb-js/mongosh.git"
  },
  "scripts": {
    "compile-ts": "tsc -p tsconfig.json",
    "test": "cross-env TS_NODE_PROJECT=../../config/tsconfig.test.json mocha -r \"../../scripts/import-expansions.js\" --timeout 60000 -r ts-node/register \"./src/**/*.spec.ts\"",
    "test-ci": "node ../../scripts/run-if-package-requested.js npm test",
    "prepublish": "npm run compile-ts",
    "lint": "eslint --report-unused-disable-directives \"./{src,test}/**/*.{js,ts,tsx}\"",
    "check": "npm run lint && depcheck --skip-missing=true"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "dependency-check": {
    "entries": [
      "src/**/*.js"
    ],
    "ignore": [
      "sinon",
      "chai",
      "uuid",
      "encoding"
    ]
  },
  "dependencies": {
    "@mongosh/errors": "1.1.0",
    "@mongosh/service-provider-core": "1.1.0",
    "@mongosh/types": "1.1.0",
    "@types/sinon-chai": "^3.2.3",
    "mongodb": "^4.1.3",
    "mongodb-connection-string-url": "^2.0.0",
    "saslprep": "mongodb-js/saslprep#v1.0.4"
  },
  "optionalDependencies": {
    "kerberos": "^2.0.0-beta.0",
    "mongodb-client-encryption": "^1.2.7",
    "os-dns-native": "^1.0.3",
    "resolve-mongodb-srv": "^1.1.0"
  },
  "gitHead": "1f9987e3eb6bf9071a245201183a4b9e51ff1616"
}
